<UserControl x:Class="FairsenseUI.Controls.WebOfFairsense"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FairsenseUI.Controls"
			 xmlns:re="clr-namespace:FairsenseUI"
			 xmlns:gif="https://github.com/XamlAnimatedGif/XamlAnimatedGif"
			 xmlns:cefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="UserControl_Loaded">
    <Grid>
		<cefSharp:ChromiumWebBrowser x:Name="Browser" FrameLoadEnd="Browser_FrameLoadEnd"/>
		<Image x:Name="waitImg" gif:AnimationBehavior.SourceUri="/Resources/timg.gif" Stretch="Fill"/>
	</Grid>
</UserControl>
